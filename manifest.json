{
  "manifest_version": 2,
  "name": "Enchanced YT",
  "description": "Auto skip youtube / youtube music ads & Download video/audio.",
  "version": "1.0",
  "icons": {
    "512": "icons/enchanced.png"
  },
  "content_scripts": [
    {
      "matches": ["https://youtube.com/*"],
      "js": ["ytdl.js", "skip.js"]
    },
    {
      "matches": ["https://music.youtube.com/*"],
      "js": ["ytm.js", "skip.js"]
    }
  ]
}
